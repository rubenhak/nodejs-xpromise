{
  "name": "the-promise",
  "version": "2.0.14",
  "description": "An extension over bluebird promise library.",
  "main": "dist/index.js",
  "types": "dist",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "build": "tsc",
    "format": "prettier --write ./src/ ./test/",
    "format-check": "prettier --write ./src/ ./test/",
    "lint": "eslint"
  },
  "author": "Ruben Hakopian <ruben.hakopian@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubenhak/nodejs-xpromise.git"
  },
  "bugs": {
    "url": "https://github.com/rubenhak/nodejs-xpromise/issues"
  },
  "homepage": "https://github.com/rubenhak/nodejs-xpromise#readme",
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "3.5.36",
    "bluebird": "3.7.2",
    "moment": "2.29.4",
    "the-lodash": "^2.0.9"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/should": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^8.23.0",
    "mocha": "^8.1.1",
    "prettier": "^2.1.0",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
